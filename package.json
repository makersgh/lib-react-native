{
  "name": "@my-app/lib-reactnative",
  "version": "1.0.2",
  "description": "library used in akuila apps",
  "homepage": "https://bitbucket.org/akuila2020/akuila-library#readme",
  "repository": {
    "type": "git",
    "url": "git+https://bitbucket.org/akuila2020/akuila-library.git"
  },
  "license": "ISC",
  "author": "Manny",
  "main": "index.js",
  "scripts": {
    "update-version": "npx react-native-version --never-amend",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "start-server": "cd ../../akuila_server && npm start",
    "test-pixel3": "adb shell wm size 1440x2960; adb shell wm density 560",
    "test-iphone5": "adb shell wm size 640x1136; adb shell wm density 326",
    "test-technopop2": "adb shell wm size 480x960; adb shell wm density 197",
    "test-huaweip8": "adb shell wm size 1080x1920; adb shell wm density 424",
    "build-release": "cd ./android && gradlew.bat clean && gradlew.bat bundleRelease && npx react-native run-android --variant=release && start ./android/app/build/outputs/apk/release ; cd .."
  },
  "peerDependencies": {
